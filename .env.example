# ===================================================================
# Performance Testing Tool - Environment Configuration
# ===================================================================
#
# Copy this file to .env and fill in your credentials
#
# ===================================================================

# GitHub Authentication (REQUIRED)
# Personal Access Token for repository cloning and Maven authentication
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_USER=your_github_username_here

# AWS Credentials (REQUIRED)
# Used for Device Farm test execution
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_DEFAULT_REGION=us-west-2

# AWS Session Token (OPTIONAL)
# Only needed if using temporary credentials
# AWS_SESSION_TOKEN=your_session_token_here
