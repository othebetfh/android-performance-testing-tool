# GitHub Authentication
# Personal Access Token for accessing private worldcoin/wld-android repo
GITHUB_PAT=your_github_personal_access_token_here

# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_DEFAULT_REGION=us-west-2

# AWS Device Farm Configuration
AWS_DEVICEFARM_PROJECT_ARN=arn:aws:devicefarm:us-west-2:ACCOUNT_ID:project/PROJECT_ID

# Optional: Device Farm device pool
DEVICEFARM_DEVICE_POOL=Top Devices

# Optional: Override default output directory
# PERFTEST_OUTPUT_DIR=./output

# Optional: Override log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# PERFTEST_LOG_LEVEL=INFO

# Optional: Override Perfetto trace processor path
# PERFETTO_PATH=/usr/local/bin/trace_processor_shell

# Optional: Override GitHub repository URL
# GITHUB_REPO_URL=https://github.com/worldcoin/wld-android
